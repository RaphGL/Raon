theme = "github_dark"
tab_width = { spaces = 4,  convert-to-space = true }

# this comment would be ignored
editor = {
	line-number = "relative"
	cursorline = true
	true-color = true

	cursor-shape = {
		insert = "bar"
		normal = "block"
		select = "block"	
	}
}

languages = { # comments are ignored until newline
	python =  { language-server = "pyright" }
}
